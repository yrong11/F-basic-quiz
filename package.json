{
  "name": "basic-quiz",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/GTB-training/F-basic-quiz.git",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html --open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "json-server --watch json-server/api.json",
    "lint": "eslint \"src/\" --cache --fix --format table",
    "format": "prettier --write \"src/**/*.{js,css,scss,html}\""
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": ">=4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "json-server": "^0.16.1",
    "lint-staged": ">=10",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.5",
    "sass": "^1.26.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint \"src/\" --cache --fix --format table",
    "*.{js,css,scss,html}": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.20.0"
  }
}
